---
import Layout from "./Layout.astro";
interface Props {
  title?: string;
  description?: string;
  image?: string;
}

const { title, description, image } = Astro.props;
---

<Layout title={title} description={description} image={image}>
  <slot />
</Layout>

<style is:global>
  text {
    user-select: none;
  }
</style>
